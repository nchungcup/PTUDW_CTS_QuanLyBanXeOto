@model List<User>
@foreach(var ld in Model)
{
    <div class="services-container-inner">
        <h3 class="about-title-2">Nhà Phát Triển</h3>
        <img src="~/img/User/@ld.UserImage" alt="Image" class="leader-image margin-bottom-15">
        <p class="about-description">@ld.HoTen</p>
        <a href="#" class="about-link about-link-2">Chi tiết</a>
    </div>
}